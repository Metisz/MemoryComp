             xmlns:local="clr-namespace:MemoryComp"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <Style x:Key="KarmaFuture" TargetType="Label">
            <Setter Property="TextElement.FontFamily" Value="/CustomFonts/#Karma Future">
                
